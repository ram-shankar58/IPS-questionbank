#include <stdio.h>
int main() {
    int n,i,sum,j;
    sum=0;
    scanf("%d",&n);
    
    int a[n];
    for(i=0;i<n;++i){
        if (i==n-1){
            scanf("%d",&a[i]);
            printf("%d",a[i]);
        }
        else{
            scanf("%d",&a[i]);
            printf("%d, ",a[i]);
        
        
        }
        sum+=a[i];
         
    }
    printf("\n%d",sum);   
    return 0;
}
